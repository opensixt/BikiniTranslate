{
    "name": "opensixt/sx-translate-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides Sixt specific changes to opensixt/BikiniTranslate",
    "keywords": ["translate"],
    "homepage": "http://www.opensixt.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitri Mansilia",
            "email": "dmitri.mansilia@sixt.com"
        },
        {
            "name": "Uwe Pries",
            "email": "uwe.pries@sixt.de"
        },
        {
            "name": "Sixt",
            "homepage": "http://www.sixt.com"
        }
    ],

    "require": {
        "php":                              ">=5.3.2",
        "symfony/symfony":                  ">=2.0,<2.2-dev",
        "sensio/framework-extra-bundle":    ">=2.0,<2.2-dev",
        "jms/serializer-bundle":            "0.9.*",
        "doctrine/fixtures":                "*",
        "doctrine/migrations":              "*",
        "opensixt/bikini-translate-bundle": "*"
    },

    "autoload": {
        "psr-0": { 
            "Opensixt\\BikiniTranslateBundle": "",
            "Doctrine\\Common\\DataFixtures": "lib/",
            "Doctrine\\DBAL\\Migrations": "lib"
        }
    },

    "target-dir": "opensixt/BikiniTranslateBundle",

    "extra": {
    }
}

